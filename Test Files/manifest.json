{
  "name": "Traffic Stop",
  "description": "Creatue popup linking to new page with statistics of animal trafficing",
  "permissions": [ "activeTab", "declarativeContent", "storage", "tabs", "<all_urls>" ],
  "version": "0.0.1",
  "background": {
    "scripts": [ "background.js", "popup.js" ],
    "persistent": false
  },
  "page_action": {
   "default_popup": "popup.html",
   "default_icon": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
  },
"icons": {
  "16": "images/get_started16.png",
  "32": "images/get_started32.png",
  "48": "images/get_started48.png",
 "128": "images/get_started128.png"
},
 
  "manifest_version": 2
}
