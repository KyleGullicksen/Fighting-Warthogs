{
  "name": "Traffic Stop",
  "version": "1.0.0",
  "description": "This extension will notify you in case you are about to make a bad purchase!",
  "options_page": "options.html",
  "permissions": [ "activeTab", "declarativeContent", "storage" ],
  "background": {
    "scripts": [ "watchdog.js" ],
    "persistent":true 
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      //"48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/go16.png",
    "32": "images/go32.png",
    "48": "images/go48.png",
    "128": "images/go128.png"
  },
  "manifest_version": 2
}
